{
	"name": "@jackshelton/qwik-analyzer",
	"version": "0.1.18",
	"type": "module",
	"main": "src/vite/plugin.js",
	"types": "src/vite/plugin.d.ts",
	"exports": {
		".": {
			"types": "./src/vite/plugin.d.ts",
			"default": "./src/vite/plugin.js"
		},
		"./napi": {
			"default": "./index.cjs"
		}
	},
	"packageManager": "pnpm@10.7.0",
	"publishConfig": {
		"access": "public"
	},
	"napi": {
		"name": "qwik-analyzer",
		"npmName": "@jackshelton/qwik-analyzer",
		"triples": {
			"defaults": false,
			"additional": [
				"x86_64-apple-darwin",
				"aarch64-apple-darwin",
				"x86_64-pc-windows-msvc"
			]
		}
	},
	"license": "MIT",
	"devDependencies": {
		"@napi-rs/cli": "^2.18.4",
		"@types/node": "^22.0.0",
		"bumpp": "^10.1.1",
		"typescript": "^5.0.0",
		"vite": "^6.3.5",
		"vitest": "^3.2.0"
	},
	"engines": {
		"node": ">= 10"
	},
	"scripts": {
		"dev": "pnpm -C ./qwik-app dev",
		"artifacts": "napi artifacts",
		"build": "tsc && napi build --platform --release --js index.cjs",
		"build:cross": "napi build --platform --release --target",
		"build:debug": "napi build --platform",
		"prepublishOnly": "napi prepublish -t npm && tsc",
		"test": "vitest run",
		"universal": "napi universal",
		"version": "napi version",
		"bump": "pnpm bumpp"
	}
}
