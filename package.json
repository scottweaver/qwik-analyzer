{
	"name": "@jackshelton/qwik-analyzer",
	"version": "0.1.8",
	"main": "src/vite/plugin.ts",
	"types": "src/vite/plugin.ts",
	"packageManager": "pnpm@10.7.0",
	"publishConfig": {
		"access": "public"
	},
	"napi": {
		"name": "qwik-analyzer",
		"triples": {
			"additional": ["aarch64-apple-darwin", "x86_64-pc-windows-msvc"]
		}
	},
	"license": "MIT",
	"devDependencies": {
		"@napi-rs/cli": "^2.18.4",
		"@types/node": "^22.0.0",
		"bumpp": "^10.1.1",
		"typescript": "^5.0.0",
		"vite": "^6.3.5",
		"vitest": "^3.2.0"
	},
	"engines": {
		"node": ">= 10"
	},
	"scripts": {
		"dev": "pnpm -C ./qwik-app dev",
		"artifacts": "napi artifacts",
		"build": "napi build --platform --release",
		"build:debug": "napi build --platform",
		"prepublishOnly": "napi prepublish -t npm",
		"test": "vitest run",
		"universal": "napi universal",
		"version": "napi version"
	},
	"optionalDependencies": {
		"@jackshelton/qwik-analyzer-win32-x64-msvc": "0.1.8",
		"@jackshelton/qwik-analyzer-darwin-x64": "0.1.8",
		"@jackshelton/qwik-analyzer-linux-x64-gnu": "0.1.8",
		"@jackshelton/qwik-analyzer-darwin-arm64": "0.1.8"
	}
}
